    "analyze": "yarn build --env.analyze",
    "build": "yarn clean-dist && node --max-old-space-size=2048 ./node_modules/.bin/webpack --bail --profile",
    "clean-test": "rimraf ./test/results ./test/coverage && yarn test",
    "clean-dist": "rimraf dist dist-source-map build",
    "commit": "git-cz",
    "coverage-report": "node ./utils/coverage-report",
    "debug-webpack": "node --inspect --debug-brk ./node_modules/.bin/webpack --env.no-devtool",
    "e2e": "yarn protractor --production-backend --suite jenkins",
    "eslint": "eslint ./app ./config ./karma ./protractor ./test ./utils ./webpack",
    "gauntlet-push": "./bin/gauntlet-push.sh",
    "json-verify": "node ./utils/json-verify.js",
    "ktest-all-no-parallel": "./bin/ktest-all --no-parallel",
    "ktest-all-watch": "./bin/ktest-all --no-parallel --no-single-run --auto-watch",
    "ktest-all": "./bin/ktest-all",
    "ktest-debug": "./bin/ktest --no-parallel --no-single-run --debug",
    "ktest-watch": "./bin/ktest --no-parallel --no-single-run --auto-watch",
    "languages-verify": "node ./utils/languages-verify.js",
    "lint": "yarn eslint && yarn tslint && yarn stylelint",
    "posttest": "yarn coverage-report",
    "preinstall": "node ./utils/version-check.js",
    "prestart": "yarn",
    "pretest": "rimraf ./test/coverage",
    "protractor": "yarn webdriver && node ./protractor/e2e",
    "protractor-babel": "yarn webdriver && babel-node ./protractor/e2e",
    "rebase-branch-and-update-fork": "./bin/rebase-branch-and-update-fork.sh",
    "sauce-job": "./bin/mk-proper-sauce-link.sh",
    "serve-dist": "./bin/start-lite-server.sh dist",
    "serve": "./bin/start-lite-server.sh dev",
    "start": "yarn serve",
    "stats": "yarn clean-dist && webpack --progress --profile --json > stats.json",
    "stylelint": "stylelint './app/**/**/*.scss'",
    "test": "yarn ktest-all",
    "tslint": "tsc --noEmit && tslint --format stylish --project ./tsconfig.json",
    "webdriver": "webdriver-manager update"
  },
